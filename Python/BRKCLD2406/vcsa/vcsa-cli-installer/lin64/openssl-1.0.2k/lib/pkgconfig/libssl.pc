prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib64
includedir=${prefix}/include

Name: OpenSSL-libssl
Description: Secure Sockets Layer and cryptography libraries
Version: 1.0.2k
Requires.private: libcrypto
Libs: -L${libdir} -lssl
Libs.private: -Wl,-z,relro -Wl,-z,now -Wl,--hash-style,both -ldl
Cflags: -I${includedir} 
